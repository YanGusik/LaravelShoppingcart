{
    "name": "mateusjatenee/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Mateus Guimaraes",
            "email": "mateus@weenside.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "illuminate/session": "~5.1",
        "illuminate/events": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Mateusjatenee\\Shoppingcart\\": "src/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mateusjatenee\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Mateusjatenee\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
